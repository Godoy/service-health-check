{
  "name": "Service Health Check",
  "version": "1.0",
  "description": "Run Health check request against a URL",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_scripts": [{
    "js": ["jquery-3.3.1.min.js", "main.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Title",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
